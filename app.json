{
  "expo": {
    "name": "VibeLearner",
    "slug": "vibe-learner",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vibelearner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/VibeLearnerIcon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sampath2001.vibelearner",
      "icon": "./assets/VibeLearnerIcon.png"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "icon": "./assets/VibeLearnerIcon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/VibeLearnerIcon.png",
        "backgroundColor": "#00B8B0"
      },
      "package": "com.sampath2001.vibelearner",
      "softInputMode": "adjustResize",
      "enableProguardInReleaseBuilds": true,
      "enableR8": true,
      "minifyEnabled": true,
      "shrinkResources": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eb0d94a8-f3f7-4a30-97da-09fa1efc5bd3"
      }
    },
    "owner": "sampath2001"
  }
}
